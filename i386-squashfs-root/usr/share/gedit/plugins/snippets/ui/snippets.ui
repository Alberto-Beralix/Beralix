        <col id="0">image/png</col>
      </row>
      <row>
        <col id="0">image/jpeg</col>
      </row>
      <row>
        <col id="0">audio</col>
      </row>
      <row>
        <col id="0">video</col>
      </row>
    </data>
  </object>
  <object class="GtkAction" id="remove_snippet_action">
    <signal name="activate" handler="on_action_remove_snippet_activated" swapped="no"/>
  </object>
