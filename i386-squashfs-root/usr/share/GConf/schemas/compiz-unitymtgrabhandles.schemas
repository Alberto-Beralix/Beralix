<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/compiz-1/plugins/unitymtgrabhandles/screen0/options/toggle_handles_key</key>
      <applyto>/apps/compiz-1/plugins/unitymtgrabhandles/screen0/options/toggle_handles_key</applyto>
      <owner>compiz-1</owner>
      <type>string</type>
      <default>Disabled</default>
      <gettext_domain>unity</gettext_domain>
      <locale name="C">
        <short>Toggle Handles</short>
        <long>Key to toggle the handles</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/unitymtgrabhandles/screen0/options/show_handles_key</key>
      <applyto>/apps/compiz-1/plugins/unitymtgrabhandles/screen0/options/show_handles_key</applyto>
      <owner>compiz-1</owner>
      <type>string</type>
      <default>Disabled</default>
      <gettext_domain>unity</gettext_domain>
      <locale name="C">
        <short>Show Handles</short>
        <long>Key to show the handles</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/unitymtgrabhandles/screen0/options/hide_handles_key</key>
      <applyto>/apps/compiz-1/plugins/unitymtgrabhandles/screen0/options/hide_handles_key</applyto>
      <owner>compiz-1</owner>
      <type>string</type>
      <default>Disabled</default>
      <gettext_domain>unity</gettext_domain>
      <locale name="C">
        <short>Hide Handles</short>
        <long>Key to hide the handles</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/compiz-1/plugins/unitymtgrabhandles/screen0/options/fade_duration</key>
      <applyto>/apps/compiz-1/plugins/unitymtgrabhandles/screen0/options/fade_duration</applyto>
      <owner>compiz-1</owner>
      <type>int</type>
      <default>150</default>
      <gettext_domain>unity</gettext_domain>
      <locale name="C">
        <short>Fade Duration</short>
        <long>How long the fade should last ( - )</long>
      </locale>
    </schema>

  </schemalist>
</gconfschemafile>
