          nao:version "2" .

    <http://www.semanticdesktop.org/ontologies/2009/02/19/nmm/metadata>
          a       nrl:GraphMetadata , nrl:DocumentGraph ;
          nao:serializationLanguage "TriG" ;
          nrl:coreGraphMetadataFor nmm: .
}
