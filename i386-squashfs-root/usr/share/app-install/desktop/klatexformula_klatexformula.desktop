[Desktop Entry]
X-AppInstall-Package=klatexformula
X-AppInstall-Popcon=4
X-AppInstall-Section=universe

Name=KLatexFormula
GenericName=KLatexFormula
Comment=Generate images from LaTeX equations
Categories=Qt;Office;
MimeType=application/x-klatexformula;application/x-klatexformula-db;
Exec=/usr/bin/klatexformula %F
Icon=_usr_share_pixmaps__klatexformula-64
Type=Application
Terminal=false
X-KDE-StartupNotify=true
X-KDE-AuthorizeAction=shell_access

X-Ubuntu-Gettext-Domain=app-install-data
