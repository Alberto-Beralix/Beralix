<?xml version="1.0"?>
<color_scheme name="Classic Onboard">
    <pane fill="#000000" fill-opacity="0.3"/>
    <pane fill="#4d4db3" fill-opacity="0.3"/>
    <pane fill="#994db3" fill-opacity="0.3"/>
    <key_group default="True" 
           opacity="1.0" 
           fill="#e5d8b2"    fill-opacity="1.0"
           hover="#e5d8b2"   hover-opacity="1.0"
           pressed="#e5d8b2" presssed-opacity="1.0"
           latched="#7f7f7f" latched-opacity="1.0"
           locked="#ff0000"  locked-opacity="1.0"
           scanned="#7373b2" scanned-opacity="1.0"
           stroke="#000000"  stroke-opacity="1.0"
           label="#000000"   label-opacity="1.0"/>
    <key_group fill="#f2e5d8">
        RCTL, LCTL, RALT, LALT, LWIN, CAPS, 
        LFSH, RTSH, NMLK, secondaryClick, middleClick
    </key_group>
    <key_group fill="#d8cca5">
        MENU, RWIN, BKSP, TAB, RTRN, 
        KPDL, KPEN, KP0, KP1, KP2, KP3, KP4, KP5, KP6, KP7, KP8, KP9,
        LEFT, RGHT, UP, DOWN, INS, DELE, HOME, END, PGUP, PGDN,
        F1, F2, F3, F4, F5, F6, F7, F8, F9, F10, F11, F12,
        Prnt, Pause, ESC, Scroll
    </key_group>
    <key_group fill="#7f7fcc">
        m0, m1, m2, m3, m4, m5, m6, m7, m8,
    </key_group>
    <key_group fill="#f27f7f">
        settings
    </key_group>
</color_scheme>

