<?xml version="1.0"?>
<color_scheme name="Granite">
    <pane fill="#000e29" fill-opacity="1"/>
    <pane fill="#202e39" fill-opacity="0.9"/>
    <pane fill="#3a424f" fill-opacity="0.9"/>
    <key_group default="True" 
           fill="#3a424f" fill-opacity="1"
           stroke="#3a424f" stroke-opacity="1"
           label="#ffffff"/>
    <key_group fill="#212936" stroke="#212936" label="#d2d6d9">
        RCTL, LCTL, RALT, LALT, LWIN, CAPS, 
        LFSH, RTSH, NMLK, secondaryClick, middleClick
        MENU, RWIN, BKSP, TAB, RTRN, 
        KPDL, KPEN, KPSU, KPDV, KPAD, KPMU,
        LEFT, RGHT, UP, DOWN, INS, DELE, HOME, END, PGUP, PGDN,
        F1, F2, F3, F4, F5, F6, F7, F8, F9, F10, F11, F12,
        Prnt, Pause, ESC, Scroll
    </key_group>
    <key_group fill="#2b2b63" stroke="#2b2b63">
        m0, m1, m2, m3, m4, m5, m6, m7, m8,
    </key_group>
    <key_group fill="#7f0000" stroke="#7f0000">
        settings
    </key_group>
</color_scheme>

