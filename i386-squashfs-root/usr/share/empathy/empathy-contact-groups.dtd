<!-- Root element. -->
<!ELEMENT contacts (account)>

<!ELEMENT account (group)+>
<!ATTLIST account 
	name CDATA #REQUIRED>

<!-- Groups in the roster. -->
<!ELEMENT group EMPTY>
<!ATTLIST group
   name CDATA #REQUIRED
   expanded CDATA #REQUIRED>
