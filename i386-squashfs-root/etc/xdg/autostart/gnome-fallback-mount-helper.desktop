[Desktop Entry]
Name=Mount Helper
Comment=Automount and autorun plugged devices
Exec=/usr/lib/gnome-settings-daemon/gnome-fallback-mount-helper
Icon=drive-optical
Terminal=false
Type=Application
Categories=
NoDisplay=true
OnlyShowIn=GNOME;Unity;
X-GNOME-Autostart-Notify=true
AutostartCondition=GNOME3 unless-session gnome-shell
X-Ubuntu-Gettext-Domain=gnome-settings-daemon
