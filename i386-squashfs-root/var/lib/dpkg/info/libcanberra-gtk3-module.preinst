#!/bin/sh

set -e

case "$1" in
  upgrade)
    if dpkg --compare-versions "$2" eq "0.26-1ubuntu1"; then
      if [ -d '/etc/X11/Xsession.d/52libcanberra-gtk3-module_add-to-gtk-modules' ]; then
        rm -rf '/etc/X11/Xsession.d/52libcanberra-gtk3-module_add-to-gtk-modules'
      fi
    fi
esac


exit 0
