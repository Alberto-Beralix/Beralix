#!/bin/sh -e

case "$1" in
  purge)
	rm -rf /var/lib/sudo
  ;;

  remove|upgrade|deconfigure)
  ;;

  failed-upgrade)
  ;;

  *)
        echo "unknown argument --> $1" >&2
        exit 0
  ;;
esac

# Automatically added by dh_installinit
if [ "$1" = "purge" ] ; then
	update-rc.d sudo remove >/dev/null
fi
# End automatically added section

