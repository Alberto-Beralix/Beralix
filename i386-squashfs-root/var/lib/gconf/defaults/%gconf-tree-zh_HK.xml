<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="desktop">
			<dir name="ibus">
				<dir name="panel">
					<entry name="custom_font">
						<local_schema short_desc="自訂字型">
							<longdesc>為語言面板自訂字型</longdesc>
						</local_schema>
					</entry>
					<entry name="use_custom_font">
						<local_schema short_desc="使用自訂字型">
							<longdesc>語言面板是否使用自訂字型</longdesc>
						</local_schema>
					</entry>
					<entry name="show_im_name">
						<local_schema short_desc="顯示輸入法名稱">
							<longdesc>在語言列上顯示輸入法名稱</longdesc>
						</local_schema>
					</entry>
					<entry name="show_icon_on_systray">
						<local_schema short_desc="在系統匣內顯示圖示">
							<longdesc>在系統匣內顯示圖示</longdesc>
						</local_schema>
					</entry>
					<entry name="lookup_table_orientation">
						<local_schema short_desc="查選表單表排列方向">
							<longdesc>查詢表單的排列方向。0 = 水平，1 = 垂直</longdesc>
						</local_schema>
					</entry>
					<entry name="y">
						<local_schema short_desc="語言面板位置">
						</local_schema>
					</entry>
					<entry name="x">
						<local_schema short_desc="語言面板位置">
						</local_schema>
					</entry>
					<entry name="show">
						<local_schema short_desc="自動隱藏">
							<longdesc>語言面板行為。0 = 嵌入選單內，1 = 自動隱藏，2 = 永遠顯示</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="general">
					<entry name="use_global_engine">
						<local_schema short_desc="使用全域輸入法">
							<longdesc>在所有的應用程式中共享同一個輸入法</longdesc>
						</local_schema>
					</entry>
					<entry name="embed_preedit_text">
						<local_schema short_desc="內嵌編輯模式">
							<longdesc>在應用程式視窗中內嵌編輯模式</longdesc>
						</local_schema>
					</entry>
					<entry name="use_system_keyboard_layout">
						<local_schema short_desc="使用系統鍵盤配置">
							<longdesc>使用系統鍵盤 (XKB) 配置</longdesc>
						</local_schema>
					</entry>
					<entry name="preload_engines">
						<local_schema short_desc="預先載入引擎">
							<longdesc>當 ibus 啟動時預先載入引擎</longdesc>
						</local_schema>
					</entry>
					<dir name="hotkey">
						<entry name="previous_engine">
							<local_schema short_desc="「上一個引擎」快捷鍵">
							</local_schema>
						</entry>
						<entry name="prev_engine">
							<local_schema short_desc="「上一個引擎」快捷鍵">
							</local_schema>
						</entry>
						<entry name="next_engine_in_menu">
							<local_schema short_desc="「下一個引擎」快捷鍵">
							</local_schema>
						</entry>
						<entry name="next_engine">
							<local_schema short_desc="「下一個引擎」快捷鍵">
							</local_schema>
						</entry>
						<entry name="trigger">
							<local_schema short_desc="觸發用快捷鍵">
								<longdesc>用來開啟或關閉輸入法的快捷鍵</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
			</dir>
		</dir>
