			<dir name="ibus">
				<dir name="panel">
					<entry name="custom_font">
						<local_schema short_desc="मनपसंत फॉन्ट">
							<longdesc>भाषा पटल करीता स्वपसंत फॉन्ट नाव</longdesc>
						</local_schema>
					</entry>
					<entry name="use_custom_font">
						<local_schema short_desc="स्वपसंत फॉन्ट वापरा">
							<longdesc>भाषा पटल करीत स्वपसंत फॉन्ट नाव वापरा</longdesc>
						</local_schema>
					</entry>
					<entry name="show_im_name">
						<local_schema short_desc="इंपुट पद्धतीचे नाव दाखवा">
							<longdesc>भाषा पट्टीवरील इंपुट पद्धतीचे नाव दाखवा</longdesc>
						</local_schema>
					</entry>
					<entry name="show_icon_on_systray">
						<local_schema short_desc="चिन्ह प्रणाली ट्रेवर दाखवा">
							<longdesc>चिन्ह प्रणाली ट्रेवर दाखवा</longdesc>
						</local_schema>
					</entry>
					<entry name="lookup_table_orientation">
						<local_schema short_desc="लुकअप टेबलचे निर्देशन">
							<longdesc>लुकअप टेबलचे निर्देशन. 0 = Horizontal, 1 = Vertical</longdesc>
						</local_schema>
					</entry>
					<entry name="y">
						<local_schema short_desc="भाषा पटलचे स्थान">
						</local_schema>
					</entry>
					<entry name="x">
						<local_schema short_desc="भाषा पटलचे स्थान">
						</local_schema>
					</entry>
					<entry name="show">
						<local_schema short_desc="स्वयं लपवा">
							<longdesc>भाषा पटलचे वर्तन. 0 = Embedded in menu, 1 = Auto hide, 2 = Always show</longdesc>
						</local_schema>
					</entry>
				</dir>
				<dir name="general">
					<entry name="enable_by_default">
						<local_schema short_desc="पूर्वनितर्धारीतपणे इंपुट पद्धत सुरू करा">
							<longdesc>ऍप्लिकेशनला इंपुट फोकस प्राप्त झाल्यावर इंपुट पद्धत पूर्वनिर्धारीतपणे सुरू करा</longdesc>
						</local_schema>
					</entry>
					<entry name="use_global_engine">
						<local_schema short_desc="ग्लोबल इंपुट पद्धत नीवडा">
							<longdesc>सर्व ऍप्लिकेशन्स् मध्ये एकसारखेच इंपुट पद्धत शेअर करा</longdesc>
						</local_schema>
					</entry>
					<entry name="embed_preedit_text">
						<local_schema short_desc="प्रिएडीट मजकूर एम्बेड करा">
							<longdesc>ऍप्लिकेशन पटलात प्रिएडीट मजकूर एम्बेड करा</longdesc>
						</local_schema>
					</entry>
					<entry name="use_system_keyboard_layout">
						<local_schema short_desc="प्रणाली कळफलक मांडणीचा वापर करा">
							<longdesc>प्रणाली कळफलक (XKB) मांडणीचा वापर करा</longdesc>
						</local_schema>
					</entry>
					<entry name="preload_engines">
						<local_schema short_desc="प्रीलोड इंजीन्स्">
							<longdesc>ibus सुरू होतेवेळी इंजीन आधिपासूनच लोड करा</longdesc>
						</local_schema>
					</entry>
					<dir name="hotkey">
						<entry name="previous_engine">
							<local_schema short_desc="मागील इंजीनचे शॉर्टकट किज्">
								<longdesc>मागील इंपुट पद्धत नीवडण्याकरीता वापरण्याजोगी शार्टकट किज्</longdesc>
							</local_schema>
						</entry>
						<entry name="prev_engine">
							<local_schema short_desc="मागील इंजीनचे शॉर्टकट किज्">
								<longdesc>मागील इंपुट पद्धत नीवडण्याकरीता वापरण्याजोगी शार्टकट किज्</longdesc>
							</local_schema>
						</entry>
						<entry name="next_engine_in_menu">
							<local_schema short_desc="पुढील इंजीनचे शार्टकट किज्">
								<longdesc>सूचीतील पुढील इंपुट पद्धत नीवडण्याकरीता वापरण्याजोगी शार्टकट किज्</longdesc>
							</local_schema>
						</entry>
						<entry name="next_engine">
							<local_schema short_desc="पुढील इंजीनचे शार्टकट किज्">
								<longdesc>सूचीतील पुढील इंपुट पद्धत नीवडण्याकरीता वापरण्याजोगी शार्टकट किज्</longdesc>
							</local_schema>
						</entry>
						<entry name="disable_unconditional">
							<local_schema short_desc="शार्टकट किज् बंद करा">
								<longdesc>इंपुट पद्धती बंद करण्यासाठी शार्टकट किज्</longdesc>
							</local_schema>
						</entry>
						<entry name="enable_unconditional">
							<local_schema short_desc="शार्टकट किज् सुरू करा">
								<longdesc>इंपुट पद्धती सुरू करण्यासाठी शार्टकट किज्</longdesc>
							</local_schema>
						</entry>
						<entry name="trigger">
							<local_schema short_desc="शॉर्टकट किज् ट्रिगर करा">
								<longdesc>इंपुट पद्धत सुरू किंवा बंद करण्यासाठी शार्टकट किज्</longdesc>
							</local_schema>
						</entry>
